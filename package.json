{
	"name": "the-typography",
	"version": "1.1.0",
	"alias": {
		"utils": "./src/js/utils"
	},
	"scripts": {
		"dev": "webpack --mode development --watch --config ./.webpack/editor.config.js && webpack --mode development --watch --config ./.webpack/customizer.config.js && webpack --mode development --watch --config ./.webpack/previewer.config.js",
		"prod": "webpack --mode production --config ./.webpack/editor.config.js && webpack --mode production --config ./.webpack/customizer.config.js && webpack --mode production --config ./.webpack/previewer.config.js",
		"copy": "webpack --config ./.webpack/release-copy.config.js",
		"replace": "webpack --config ./.webpack/release-replace.config.js",
		"remove": "rm _extras/release/_replace.js _extras/release/_copy.js",
		"release": "npm run prod && npm run copy && npm run replace && npm run remove",
		"type": "tsc"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.4.4",
		"@types/jquery": "^3.3.29",
		"@types/lodash": "^4.14.133",
		"@types/node": "^12.0.4",
		"@types/react-color": "^3.0.0",
		"@types/react-dom": "^16.8.4",
		"@types/react-select": "^2.0.19",
		"@types/tinycolor2": "^1.4.2",
		"@types/uuid": "^3.4.4",
		"@types/webfontloader": "^1.6.29",
		"@wordpress/block-editor": "^2.1.0",
		"@wordpress/block-serialization-default-parser": "^3.2.0",
		"@wordpress/blocks": "^6.3.0",
		"@wordpress/components": "^7.4.0",
		"@wordpress/editor": "^9.3.0",
		"@wordpress/element": "^2.4.0",
		"@wordpress/hooks": "^2.3.0",
		"@wordpress/i18n": "^3.4.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.3",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"lodash": "^4.17.11",
		"mini-css-extract-plugin": "^0.7.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"string-replace-loader": "^2.1.1",
		"style-loader": "^0.23.1",
		"stylus": "^0.54.5",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.5.1",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2"
	},
	"dependencies": {
		"@material-ui/core": "^3.9.0",
		"@material-ui/icons": "^4.0.1",
		"@material-ui/lab": "^3.0.0-alpha.30",
		"@material-ui/styles": "^4.0.1",
		"immer": "^3.1.2",
		"react-color": "^2.17.3",
		"react-select": "^3.0.3",
		"react-tiny-popover": "^3.4.2",
		"tinycolor2": "^1.4.1",
		"uuid": "^3.3.2",
		"webfontloader": "^1.6.28"
	},
	"wp": {
		"plugin_name": "The Typography for Gutenberg",
		"plugin_uri": "https://wordpress.org/plugins/the-typography/",
		"plugin_description": "Add Typography to your Gutenberg site using Google Fonts.",
		"plugin_author": "melonpan",
		"plugin_version": "1.1.0",
		"plugin_tested_up_to": "5.2",
		"plugin_requires_at_least": "5.1",
		"plugin_php_version": "7.1",
		"plugin_tags": "gutenberg, blocks, typography, text, fonts, customizer"
	}
}
